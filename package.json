{
    "name": "php-class-locator",
    "displayName": "PHP Class Locator",
    "description": "It is used to open class from locator like $this->get('path-to-locator-class').",
    "version": "0.0.1",
    "publisher":"MohsinKhanShuvo",
    "license":"MIT",
    "engines": {
        "vscode": "^1.89.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:php"
    ],
    "main": "./extension.js",
    "contributes": {
        "languages": [
            {
                "id": "php",
                "extensions": [
                    ".php"
                ]
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mkshuvo/php-class-locator.git"
    },
    "icon": "pcl.png",
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/vscode": "^1.89.0",
        "@types/mocha": "^10.0.6",
        "@types/node": "18.x",
        "eslint": "^8.57.0",
        "typescript": "^5.4.5",
        "@vscode/test-cli": "^0.0.9",
        "@vscode/test-electron": "^2.3.9"
    }
}